{
  "name": "naga-codex",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack -p 3001",
    "build": "npm run pilot:analyze && next build",
    "build:analyze": "ANALYZE=true npm run build",
    "build:production": "NODE_ENV=production npm run build",
    "start": "next start",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "pilot:init": "node scripts/init-pilot.js",
    "pilot:analyze": "node scripts/analyze-project.js",
    "pilot:optimize": "node scripts/optimize-build.js",
    "pilot:enhance": "node scripts/cyberpunk-enhance.js",
    "pilot:global": "npm run pilot:init && npm link system-pilot-config",
    "pilot:performance": "node scripts/performance-monitor.js",
    "pilot:quality": "npm run lint && npm run typecheck && npm run test",
    "pilot:deploy": "npm run pilot:quality && npm run build:production",
    "cyberpunk:dev": "NODE_ENV=development CYBERPUNK_MODE=true npm run dev",
    "cyberpunk:build": "CYBERPUNK_MODE=true npm run build",
    "prepare": "husky install || echo 'Husky not installed'"
  },
  "dependencies": {
    "@calcom/embed-react": "^1.5.3",
    "@next/bundle-analyzer": "^16.1.1",
    "@radix-ui/react-slot": "^1.2.4",
    "@tailwindcss/typography": "^0.5.19",
    "@types/gsap": "^3.0.0",
    "@types/ws": "^8.18.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "compression": "^1.7.5",
    "critters": "^0.0.24",
    "framer-motion": "^12.23.24",
    "gsap": "^3.14.2",
    "lucide-react": "^0.554.0",
    "next": "16.1.1",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "tailwind-merge": "^3.4.0",
    "web-vitals": "^4.2.4",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.3.0",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5",
    "zod": "^3.22.0"
  }
}
